[program:gunicorn]
command=bash -c "cd /home/artisan/django_venv && source bin/activate && gunicorn -c /etc/opt/${PROJECT_NAME}/settings/gunicorn.conf.py ${django_project_name}.wsgi:application &"
numprocs=11
stdout_logfile=/dev/fd/1
stdout_logfile_maxbytes=0
environment=PYTHONPATH=/etc/opt/${PROJECT_NAME}/settings/:/opt/${PROJECT_NAME}/