[program:gunicorn]
command=/usr/local/bin/gunicorn -c /etc/opt/${project_name}/settings/gunicorn.conf.py ${django_project_name}.wsgi:application &
numprocs=1
stdout_logfile=/dev/fd/1
stdout_logfile_maxbytes=0
environment=PYTHONPATH=/etc/opt/${project_name}/settings/:/opt/${project_name}/

[program:djangoq]
command=/usr/local/bin/python /opt/${project_name}/manage.py qcluster
numprocs=1
stdout_logfile=/dev/fd/1
stdout_logfile_maxbytes=0
environment=PYTHONPATH=/etc/opt/${project_name}/settings/:/opt/${project_name}/